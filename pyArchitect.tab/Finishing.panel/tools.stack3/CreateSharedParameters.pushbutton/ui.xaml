<Window xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Add Shared Parameters"
        Width="400" ResizeMode="NoResize" SizeToContent="Height">
    <StackPanel Margin="10">
        <Label Content="Choose Shared Parameter Set to add to the current Project" Margin="0,0,-0,0" Height="25" />
        <CheckBox x:Name="checkbox1_value" Content="Finishing Parameters" IsChecked="True"/>
        <CheckBox x:Name="checkbox2_value" Content="GOST Schedule Parameters"/>
        <Button Content="Add Shared Parameters" Height="20" Margin="0,10,0,0" Click="click" />
    </StackPanel>
</Window>